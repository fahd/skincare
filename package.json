{
  "name": "skin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "client": " webpack-dev-server --config utils/webpack.dev.js -d --watch",
    "mock-db": "yarn schema && yarn seed",
    "fresh-server": "yarn schema && yarn seed && yarn server",
    "schema": "psql postgres < server/db/seed/sql/schema.sql",
    "seed-types": "psql postgres < server/db/seed/sql/tags.sql",
    "seed-data": "babel-node server/db/seed/seedData",
    "seed-tags": "babel-node server/db/seed/seedTags",
    "seed-app": "psql postgres < server/db/seed/sql/mock_data.sql",
    "dev-build": "webpack --config webpack.js",
    "dev-server": "nodemon --ignore 'client/' --exec npx babel-node server/server.js",
    "prod-build": "NODE_ENV=production webpack --config webpack.js",
    "prod-server": "NODE_ENV=production npx babel-node server/server.js",
    "webpack-dev": "concurrently \"yarn:client\" \"yarn:dev-server\"",
    "dev": "yarn dev-build && yarn dev-server",
    "prod": "yarn prod-build && yarn prod-server",
    "start": "yarn prod-server"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.4",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.18.1",
    "apollo-server-express": "^2.18.1",
    "aws-sdk": "^2.799.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "css-loader": "^4.3.0",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "pg": "^8.3.3",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.3",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^1.3.0"
